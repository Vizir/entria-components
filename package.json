  },
  "main": "lib/index.js",
  "module": "src/index.js",
  "jsnext:main": "src/index.js",
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "babel-cli": "^6.24.1",
    "babel-plugin-material-ui": "^0.3.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "gh-pages": "^1.0.0",
    "lorem-ipsum": "^1.0.4",
    "material-ui": "^0.18.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.1.1",
    "react-tap-event-plugin": "^2.0.1"
  },
  "dependencies": {
    "prop-types": "^15.5.10"
  },
  "peerDependencies": {
    "material-ui": ">=0.18.3",
    "react": ">=0.14",
    "react-dom": ">=0.14",
    "react-router-dom": ">=4.1.1",
    "react-tap-event-plugin": ">=2.0.1"
  },
  "scripts": {
    "build": "babel src -d lib",
    "start": "start-storybook -p 6006 -c storybook",
    "demo:build": "build-storybook -c storybook",
    "demo:gh-pages": "gh-pages -d storybook-static/",
    "demo:publish": "npm run demo:build && npm run demo:gh-pages",
    "prerelease": "npm run test && npm run build",
    "release:patch": "npm run prerelease && npm version patch && git push --follow-tags && npm publish",
    "release:minor": "npm run prerelease && npm version minor && git push --follow-tags && npm publish",
    "release:major": "npm run prerelease && npm version major && git push --follow-tags && npm publish"
  }
